{
  "name": "@mycoder-mcp/mcp-server-agent",
  "description": "Agent MCP server for MyCoder-MCP",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "clean": "rimraf dist",
    "clean:all": "rimraf node_modules dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "agent",
    "ai-tools"
  ],
  "author": "Ben Houston",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "consola": "^3.2.3",
    "rimraf": "^6.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/uuid": "^9.0.8",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}
