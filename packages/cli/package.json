{
  "name": "@mycoder-mcp/cli",
  "description": "MyCoder-MCP CLI - A Model Context Protocol host that runs on the command line",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "mycoder": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "clean": "rimraf dist",
    "clean:all": "rimraf node_modules dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cli",
    "ai-tools"
  ],
  "author": "Ben Houston",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "c12": "^1.6.1",
    "citty": "^0.1.5",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "rimraf": "^6.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}